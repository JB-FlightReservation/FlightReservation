<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.joongbu.flight_reservation.mapper.SignupMapper">

	
	<insert id="insert" parameterType="SignupDto">
		INSERT INTO customer
			(ct_no, ct_id,ct_pw,ct_name,ct_email,ct_phone,ct_birth,ct_gender,ct_create,oc_no)
			VALUES
			(#{ctNo}, #{ctId},#{ctPw},#{ctName}, #{ctEmail}, #{ctPhone},#{ctBirth},#{ctGender},#{ctCreate},#{ocNo})
	</insert>
	
  		
</mapper>

